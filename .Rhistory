}
"B_(0|0)_restr" <- function(j, p, drug){
as.numeric(c(max(0, `F_(0|0)`(j, drug) - `c_(0|0)`(p, drug) * `s_k_00`(j, drug)), min(1, `F_(0|0)`(j, drug) + `c_(0|0)`(p, drug) * `s_k_00`(j, drug))))
